<%= form_for @student do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :date_of_birth %>
  <%= f.text_field :date_of_birth %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :email_address %>
  <%= f.text_field :email_address %>
  <%= f.submit %>
<% end %>
